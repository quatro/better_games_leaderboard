.row{style:'padding-left:15px; padding-right:15px;'}
  .col-xs-12
    %h3 2018 Online Qualifier
    %hr

    =render 'leaderboard/brought_by'


    .row
      .col-md-3.col-xs-12
        = semantic_form_for Filter.new({sort:0, competition:4, division:18}) do |f|
          = f.input :competition, label: false, as: :select, collection: Filter.competition_options, include_blank: false, input_html:{class:'form-control'}
          = f.input :division, label: false, as: :select, collection: Filter.agoq_division_options, include_blank: false, input_html:{class:'form-control'}
          -#= f.input :regional, label: false, as: :select, collection: Filter.regional_options, include_blank: false, input_html:{class:'form-control'}
          -#= f.input :fittest, label: 'Fittest In', as: :select, collection: Filter.fittest_options, include_blank: false
          = f.input :sort, label: false, as: :select, collection: Filter.online_qualifier_options, include_blank: false, input_html:{class:'form-control'}
          = f.input :year, label: false, as: :select, collection: Filter.year_options, include_blank: false, input_html:{class:'form-control'}
#results


=render 'leaderboard/js'
